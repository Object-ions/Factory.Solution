@{
  Layout = "_Layout";
}

@model Factory.Models.Machine;

<div class ="container">

  <h2>Delete Profile</h2>
  <hr />
  <h3>Are you sure you want to delete this profile:</h3>
  @Html.DisplayFor(model => model.Type)
  @using (Html.BeginForm())
  {
    <input type="submit" value="Yes, delete profile" />
    }
    @Html.ActionLink("Back to machine's profile", "Details", new { id = Model.MachineId })

</div>